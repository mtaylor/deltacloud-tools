package org.jboss.deltacloud.client.model;

public class Image 
{
	private String id;
	
	private String ownerId;
	
	private String name;
	
	private String description;
	
	private String architecture;
	
	public Image()
	{
	}
	
	public String getId() 
	{
		return id;
	}

	public String getOwnerId() 
	{
		return ownerId;
	}

	public String getName() 
	{
		return name;
	}

	public String getDescription()
	{
		return description;
	}

	public String getArch() 
	{
		return architecture;
	}
}
