package org.jboss.deltacloud.client.model;

import java.util.List;

public class Instance extends DeltaCloudObject
{	
	private static final long serialVersionUID = 1L;

	private String ownerId;
	
	private String name;
	
	private String image;
	
	private String flavor;
	
	private String realm;
	
	private String state;
	
	private List<String> publicAddresses;
	
	private List<String> privateAddresses;
	
	public Instance()
	{
		
	}
	
	public String getOwnerId() {
		return ownerId;
	}

	public void setOwnerId(String ownerId) {
		this.ownerId = ownerId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getImage() {
		return image;
	}

	public void setImage(String image) {
		this.image = image;
	}

	public String getFlavor() {
		return flavor;
	}

	public void setFlavor(String flavor) {
		this.flavor = flavor;
	}

	public String getRealm() {
		return realm;
	}

	public void setRealm(String realm) {
		this.realm = realm;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public List<String> getPublicAddresses() {
		return publicAddresses;
	}

	public void setPublicAddresses(List<String> publicAddresses) {
		this.publicAddresses = publicAddresses;
	}

	public List<String> getPrivateAddresses() {
		return privateAddresses;
	}

	public void setPrivateAddresses(List<String> privateAddresses) {
		this.privateAddresses = privateAddresses;
	}
	
	
	
}
