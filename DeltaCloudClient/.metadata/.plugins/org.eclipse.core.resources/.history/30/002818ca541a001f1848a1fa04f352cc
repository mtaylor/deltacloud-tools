package org.jboss.deltacloud.client;

import java.net.URL;
import java.util.List;

import org.jboss.deltacloud.client.model.Flavor;
import org.jboss.deltacloud.client.model.Instance;

public class RESTClient 
{
	public static void main(String[] args) throws Exception
	{
		DeltaCloudClient api = new DeltaCloudClient(new URL("http://localhost:3003/api"), "mockuser", "mockpassword");
		Instance instance = api.listInstances("inst3");
		
		System.out.println(instance.getPublicAddresses().size());
		
	}
}
