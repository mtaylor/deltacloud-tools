package org.jboss.deltacloud.client.model;

public class Flavor 
{
	private String id;
	
	private String arch;
	
	private int memory;
	
	private int storage;
	
	public Flavor()
	{	
	}
	
	public String getId() 
	{
		return id;
	}

	public String getArch() 
	{
		return arch;
	}

	public int getMemory() 
	{
		return memory;
	}

	public int getStorage() 
	{
		return storage;
	}
	
	
}
