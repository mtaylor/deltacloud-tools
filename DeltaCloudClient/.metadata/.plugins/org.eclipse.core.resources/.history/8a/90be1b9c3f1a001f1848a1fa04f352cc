package org.jboss.deltacloud.client.model;

import java.util.List;

public class Realm 
{
	private String id;
	
	private String name;
	
	private String state;
	
	private int limit;
	
	private List<Instance> instances;

	public String getId() 
	{
		return id;
	}

	public String getName() 
	{
		return name;
	}

	public String getState() 
	{
		return state;
	}

	public int getLimit() 
	{
		return limit;
	}
	
	public List<Instance> listInstances()
	{
		return instances;
	}
	
	
}
